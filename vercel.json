{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "oumamie.xyz" }],
      "destination": "https://www.oumamie.xyz/:path*",
      "permanent": true
    }
  ],
  "crons": [
    {
      "path": "/api/cron/job-alerts",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/check-jobs",
      "schedule": "0 7 * * 1,3"
    }
  ]
}
